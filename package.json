{
  "name": "cosell",
  "version": "0.0.0",
  "scripts": {
    "dev-serve": "vite",
    "dev": "run-p dev-serve watch:prettier watch:eslint",
    "start": "vite",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode dev",
    "build:stg": "tsc && vite build --mode stg",
    "serve": "vite preview",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format",
    "type-check": "tsc",
    "watch:prettier": "onchange -d 100 \"**/*\" -- prettier --config ./.prettierrc --write --ignore-unknown {{changed}}",
    "watch:eslint": "onchange -d 200 \"**/*\" -- npx eslint {{changed}}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/styles": "^4.11.4",
    "@mui/icons-material": "^5.2.5",
    "@mui/lab": "^5.0.0-alpha.64",
    "@mui/material": "^5.0.4",
    "@mui/styles": "^5.2.3",
    "@react-pdf-viewer/core": "^3.1.2",
    "@react-pdf-viewer/default-layout": "^3.1.2",
    "@react-pdf-viewer/zoom": "^3.1.2",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/history": "^4.7.9",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-loader-spinner": "^4.0.0",
    "@types/react-router": "^5.1.17",
    "axios": "^0.24.0",
    "date-fns": "^2.28.0",
    "downshift": "^6.1.7",
    "file-saver": "^2.0.5",
    "formik": "^2.2.9",
    "html-react-parser": "^1.4.5",
    "material-ui-chip-input": "^1.1.0",
    "pdf-viewer-reactjs": "^2.2.3",
    "pdfjs-dist": "2.12.313",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-bootstrap": "^2.0.0",
    "react-cookie": "^4.1.1",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "^17.0.2",
    "react-easy-crop": "^4.0.1",
    "react-file-viewer": "^1.2.1",
    "react-ga4": "^1.4.1",
    "react-loading-overlay": "^1.0.1",
    "react-pdf": "^5.7.1",
    "react-redux": "^7.2.5",
    "react-responsive": "^9.0.0-beta.6",
    "react-router": "5.2.1",
    "react-router-dom": "^5.3.0",
    "react-spinners": "^0.11.0",
    "react-table": "^7.7.0",
    "redux-form": "^8.3.7",
    "storybook-builder-vite": "^0.1.5",
    "universal-cookie": "^4.0.4",
    "use-file-picker": "^1.4.1",
    "vite": "^2.6.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
